{
  "name": "asci-numbers",
  "version": "1.0.0",
  "description": "Convert numbers to ASCI art text.",
  "main": "lib/asciNumbers.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "compile:watch": "babel -w -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register",
    "test:coverage": "node cover.js && cat lcov.info | coveralls && rm lcov.info",
    "release:patch": "npm test && npm run compile && npm version patch && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/machaj/asci-numbers.git"
  },
  "keywords": [
    "ASCII art"
  ],
  "author": "jan.machy@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/machaj/asci-numbers/issues"
  },
  "homepage": "https://github.com/machaj/asci-numbers#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "expect": "^1.14.0",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
